<%- include("./partials/head.ejs") %>

<div class="grid-container">
  <% for (let i in logs) { %>
  <div class="card">
    <a href="/logs/<%= logs[i].id%>"><h3><%= logs[i].date %></h3></a>
    <span class="card-detail"><%= logs[i].time %> </span>
    <span class="card-detail"><%= logs[i].mood %> </span>
    <span class="card-detail"><%= logs[i].description %> </span>
    <span class="card-detail"><%= logs[i].tags %> </span>
  </div>
  <% } %>
</div>

<%- include("./partials/footer.ejs") %>